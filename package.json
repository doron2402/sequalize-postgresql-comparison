  },
  "homepage": "https://github.com/doron2402/sequalize-postgresql-comparison#readme",
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^3.0.1",
    "fastify": "^5.6.0",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "@types/node": "^24.3.3",
    "@types/pg": "^8.15.5",
