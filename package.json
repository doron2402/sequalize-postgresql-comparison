{
  "name": "sequalize-postgresql-comparison",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "check": "biome check .",
    "format": "biome format --write .",
    "migrate": "node -r ts-node/register ./node_modules/.bin/sequelize-cli db:migrate",
    "migrate:undo": "node -r ts-node/register ./node_modules/.bin/sequelize-cli db:migrate:undo",
    "migrate:undo:all": "node -r ts-node/register ./node_modules/.bin/sequelize-cli db:migrate:undo:all",
    "seed": "node -r ts-node/register ./node_modules/.bin/sequelize-cli db:seed:all",
    "seed:undo": "node -r ts-node/register ./node_modules/.bin/sequelize-cli db:seed:undo:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/doron2402/sequalize-postgresql-comparison.git"
  },
